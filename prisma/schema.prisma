generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id            String   @id @default(cuid())
  domain        String   @unique
  isWordPress   Boolean?
  wpConfidence  Float?
  contactEmail  String?
  contactSource String?
  urlDiscovered String?
  createdAt     DateTime @default(now())
}
